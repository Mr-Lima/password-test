{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug local",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/src/index.js",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "smartStep": false,
      "preLaunchTask": null,
      "runtimeArgs": [
        "--nolazy"
      ],
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": []
    }
  ]
}